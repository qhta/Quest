        <member name="T:QuestIMP.WorkbookImporter">
            <summary>
            Class for import Excel Workbooks.
            </summary>
        </member>
        <member name="M:QuestIMP.WorkbookImporter.OpenWorkbook(System.String)">
            <summary>
            Opens an Excel workbook from the specified file and returns the IWorkbook instance.
            </summary>
            <param name="fileName">Filename of the workbook</param>
        </member>
        <member name="M:QuestIMP.WorkbookImporter.ImportProjectQuality(Syncfusion.XlsIO.IWorkbook,QuestIMP.WorkbookInfo)">
            <summary>
            Imports project quality data from the provided workbook information. Workbook must be opened prior to calling this method.
            </summary>
            <param name="workbook">Opened Excel workbook interface</param>
            <param name="workbookInfo">Info about workbook with worksheet info collection.</param>
            <returns>Project quality object</returns>
        </member>
        <member name="M:QuestIMP.WorkbookImporter.ImportWorksheets(Syncfusion.XlsIO.IWorkbook,QuestIMP.WorkbookInfo)">
            <summary>
            Gets the list of worksheets. Specifies if they contain questionnaires.
            </summary>
            <param name="workbook">Opened Excel workbook interface</param>  
            <param name="workbookInfo">Info about workbook with worksheet info collection.</param>
            <returns>List of <see cref="T:Quest.DocumentQuality"/>. Can be empty.</returns>
            <exception cref="T:System.InvalidOperationException">Raised when workbook is null or is not opened previously.</exception>
        </member>
        <member name="M:QuestIMP.WorkbookImporter.ImportWorksheetsAsync(Syncfusion.XlsIO.IWorkbook,QuestIMP.WorkbookInfo)">
            <summary>
            Asynchronously gets the list of worksheets from previously opened workbook. 
            </summary>
            <param name="workbook">Opened Excel workbook interface</param>  
            <param name="workbookInfo">Info about workbook with worksheet info collection.</param>
            <returns>Asynchronously filled list of <see cref="T:Quest.DocumentQuality"/>. Can be empty.</returns>
        </member>
        <member name="M:QuestIMP.WorkbookImporter.ImportWorksheetInfo(Syncfusion.XlsIO.IWorksheet,QuestIMP.WorksheetInfo)">
            <summary>
            Gets a single document quality object.
            </summary>
            <param name="worksheet">Interface for opened Excel worksheet</param>
