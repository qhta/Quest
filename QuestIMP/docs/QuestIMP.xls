        <member name="T:QuestIMP.WorkbookHelper">
            <summary>
            Class with helper methods for workbook processing.
            </summary>
        </member>
        <member name="M:QuestIMP.WorkbookHelper.SplitRange(System.String)">
            <summary>
            Splits a range string into its start and end address.
            </summary>
            <remarks>This method assumes that the input string is properly formatted as "start:end". If the input does
            not meet this format, the behavior may be undefined or an exception may be thrown.</remarks>
            <param name="range">A string representing a range, formatted as "start:end", where "start" and "end" are substrings separated by a
            colon (:).</param>
            <returns>A tuple containing the start and end cell address of the range.</returns>
        </member>
        <member name="M:QuestIMP.WorkbookHelper.GetCellRowIndex(System.String)">
            <summary>
            Returns zero based row index for the given cell address.
            </summary>
            <param name="cellAddress"></param>
            <returns></returns>
        </member>
        <member name="M:QuestIMP.WorkbookHelper.GetCellColumnIndex(System.String)">
            <summary>
            Returns zero based column index for the given cell address.
            </summary>
            <param name="cellAddress"></param>
            <returns></returns>
        </member>
        <member name="T:QuestIMP.WorkbookImporter">
            <summary>
            Class for import Excel Workbooks.
            </summary>
        </member>
        <member name="M:QuestIMP.WorkbookImporter.OpenWorkbook(System.String)">
            <summary>
            Opens an Excel workbook from the specified file and returns the IWorkbook instance.
            </summary>
            <param name="fileName">Filename of the workbook</param>
        </member>
        <member name="M:QuestIMP.WorkbookImporter.ImportProjectQuality(Syncfusion.XlsIO.IWorkbook,QuestIMP.WorkbookInfo)">
            <summary>
            Imports project quality data from the provided workbook information. Workbook must be opened prior to calling this method.
            </summary>
            <param name="workbook">Opened Excel workbook interface</param>
            <param name="workbookInfo">Info about workbook with worksheet info collection.</param>
            <returns>Project quality object</returns>
        </member>
        <member name="M:QuestIMP.WorkbookImporter.ImportProjectQualityAsync(Syncfusion.XlsIO.IWorkbook,QuestIMP.WorkbookInfo,Quest.ProjectQuality,System.AsyncCallback)">
            <summary>
            Retrieves data from workbook and populates the quality view model asynchronously.
            </summary>
            <param name="workbook">Opened Excel workbook</param>
            <param name="workbookInfo">Workbook info object</param>
            <param name="projectQuality">Project quality view model</param>
            <param name="callback">Method to invoke when a worksheet is imported</param>
            <returns></returns>
        </member>
        <member name="M:QuestIMP.WorkbookImporter.ImportScaleTable(Syncfusion.XlsIO.IWorksheet,QuestIMP.WorksheetInfo)">
            <summary>
            Get the grade scale from the given worksheet.
            </summary>
            <param name="worksheet">Excel worksheet with scale table</param>  
            <param name="worksheetInfo">Info about worksheet with scale range recognized.</param>
            <returns>List of <see cref="T:Quest.DocumentQuality"/>. Can be empty.</returns>
        </member>
        <member name="M:QuestIMP.WorkbookImporter.ImportDocumentQualities(Syncfusion.XlsIO.IWorkbook,QuestIMP.WorkbookInfo)">
            <summary>
            Imports the list of worksheets containing questionnaires.
            </summary>
            <param name="workbook">Opened Excel workbook interface</param>  
            <param name="workbookInfo">Info about workbook with worksheet info collection.</param>
            <returns>List of <see cref="T:Quest.DocumentQuality"/>. Can be empty.</returns>
        </member>
        <member name="M:QuestIMP.WorkbookImporter.ImportDocumentQualitiesAsync(Syncfusion.XlsIO.IWorkbook,QuestIMP.WorkbookInfo)">
            <summary>
            Asynchronously imports the list of worksheets containing questionnaires. 
            </summary>
            <param name="workbook">Opened Excel workbook interface</param>  
            <param name="workbookInfo">Info about workbook with worksheet info collection.</param>
            <returns>Asynchronously filled list of <see cref="T:Quest.DocumentQuality"/>. Can be empty.</returns>
        </member>
        <member name="M:QuestIMP.WorkbookImporter.ImportDocumentQuality(Syncfusion.XlsIO.IWorksheet,QuestIMP.WorksheetInfo)">
            <summary>
            Imports a single document quality object from a worksheet containing questionnaire.
            </summary>
            <param name="worksheet">Interface for opened Excel worksheet</param>
            <param name="worksheetInfo">Info about worksheet info.</param>
            <returns><see cref="T:Quest.DocumentQuality"/> object.</returns>
        </member>
        <member name="M:QuestIMP.WorkbookImporter.ImportDocumentQualityAsync(Syncfusion.XlsIO.IWorksheet,QuestIMP.WorksheetInfo)">
            <summary>
            Asynchronously imports a single document quality object from a worksheet containing questionnaire.
            </summary>
            <param name="worksheet">Interface for opened Excel worksheet</param>
            <param name="worksheetInfo">Info about worksheet info.</param>
            <returns>Task with <see cref="T:Quest.DocumentQuality"/> result.</returns>
        </member>
        <member name="M:QuestIMP.WorkbookImporter.ImportQuestionnaire(Syncfusion.XlsIO.IWorksheet,QuestIMP.WorksheetInfo,Quest.DocumentQuality)">
            <summary>
            Imports the questionnaire table from the given worksheet into the provided document quality object.
            The worksheet must have a defined questionnaire range.
            </summary>
            <param name="worksheet"></param>
            <param name="worksheetInfo"></param>
            <param name="documentQuality"></param>
            <returns></returns>
        </member>
        <member name="M:QuestIMP.WorkbookImporter.GetDocumentTitle(Syncfusion.XlsIO.IWorksheet,QuestIMP.WorksheetInfo)">
            <summary>
            Gets the document title from the given worksheet by searching for the first non-empty cell over the weights table.
            </summary>
            <param name="worksheet"></param>
            <param name="worksheetInfo"></param>
            <returns></returns>
        </member>
        <member name="M:QuestIMP.WorkbookImporter.GetFactorWeights(Syncfusion.XlsIO.IWorksheet,QuestIMP.WorksheetInfo,Quest.DocumentQuality)">
            <summary>
            Gets the factor weights from the given worksheet weights range.
            </summary>
            <param name="worksheet"></param>
            <param name="worksheetInfo"></param>
            <param name="documentQuality"></param>
            <returns></returns>
        </member>
