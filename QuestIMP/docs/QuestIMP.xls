        <member name="M:QuestIMP.XlsImporter.GetWorksheets(Syncfusion.XlsIO.IWorkbook)">
            <summary>
            Gets the list of worksheets. Specifies if they contain questionnaires.
            </summary>
            <param name="workbook">Interface for the Excel worksheet. If null, then previously opened Workbook is used.</param>
            <returns>List of <see cref="T:QuestIMP.WorksheetInfo"/>. Can be empty.</returns>
            <exception cref="T:System.InvalidOperationException">Raised when workbook is null and not opened previously.</exception>
        </member>
        <member name="M:QuestIMP.XlsImporter.GetWorksheetsAsync">
            <summary>
            Asynchronously gets the list of worksheets from previously opened workbook. Fills the provided collection.
            </summary>
            <returns>Asynchronously filled list of <see cref="T:QuestIMP.WorksheetInfo"/>. Can be empty.</returns>
            <exception cref="T:System.InvalidOperationException">Raised when workbook is null and not opened previously.</exception>
        </member>
