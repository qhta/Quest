            <returns><see cref="T:Quest.DocumentQuality"/> object.</returns>
        </member>
        <member name="M:QuestIMP.WorkbookImporter.ImportWorksheetInfoAsync(Syncfusion.XlsIO.IWorksheet,QuestIMP.WorksheetInfo)">
